@model System.Data.DataTable
@using System.Data
@{
    ViewBag.Title = "QLL";
    Layout = "~/Views/Shared/_LayoutBackend.cshtml";
}

<div class="main-wrapper">
    <div class="container">

        <!-- Start breadcrumb section  -->
        <div class="row">
            <div class="col-lg-12">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/webmaster/index">Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Quản lý Loại</li>
                    </ol>
                </nav>
            </div>
        </div>

        <!-- Start table basic -->
        <div class="basic-table">
            <div class="row">
                <div class="col-lg-12">
                    <div class="single-table">
                        <div class="section-head d-flex justify-content-between">
                            <div class="title">
                                <h4>Danh Sách Loại</h4>
                            </div>
                            <div class="align-self-center">
                                <a class="btn btn-success" href="/webmaster/CSQLL">Thêm</a>
                            </div>
                        </div>
                        <div class="section-body">
                            <div class="table-responsive">
                                <table class="table table-hover progress-table text-white text-center" border="0" style="border-width:0px;border-style:None;border-collapse:collapse;">
                                    <thead>
                                        <tr>
                                            <th>ID</th>
                                            <th>Type of Work</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (System.Data.DataRow row in Model.Rows)
                                        {
                                            <tr>
                                                <td>@row.ItemArray[0].ToString()</td>
                                                <td>@row.ItemArray[1].ToString()</td>
                                                <td><a href="/Webmaster/deletetype/@row.ItemArray[0].ToString()"><img src="~/Images/icon/if_f-cross_256_282471.png" /></a></td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

